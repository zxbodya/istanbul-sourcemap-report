{
  "name": "istanbul-sourcemap-report",
  "version": "0.1.0",
  "description": "Utility to generate coverage reports for sources from compiled code coverage using SourceMaps",
  "keywords": [
    "source-map",
    "istanbul",
    "coverage"
  ],
  "author": "Bogdan Savluk <savluk.bogdan@gmail.com>",
  "contributors": [
    {
      "name": "kitsonk",
      "email": "me@kitsonkelly.com"
    },
    {
      "name": "bryanforbes",
      "email": "bryan@reigndropsfall.net"
    }
  ],
  "bugs": {
    "url": "https://github.com/zxbodya/istanbul-sourcemap-report"
  },
  "license": "BSD-3-Clause",
  "main": "./lib/index.js",
  "bin": {
    "istanbul-sourcemap-report": "./lib/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zxbodya/istanbul-sourcemap-report.git"
  },
  "scripts": {
    "lint": "eslint ./lib/",
    "test": "rimraf tmp && mkdirp tmp && ./node_modules/jasmine/bin/jasmine.js",
    "cover": "rimraf coverage && rimraf tmp && mkdirp tmp  && istanbul cover -x \"*.test.js\" ./node_modules/jasmine/bin/jasmine.js"
  },
  "dependencies": {
    "istanbul": "^0.4.4",
    "minimatch": "^3.0.2",
    "source-map": ">=0.5.6"
  },
  "devDependencies": {
    "codecov.io": "0.1.6",
    "eslint": "^2.2.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.7.0",
    "eslint-plugin-jsx-a11y": "^1.2.0",
    "eslint-plugin-react": "^5.0.1",
    "istanbul": "^0.4.4",
    "jasmine": "^2.4.1",
    "jasmine-core": "^2.4.1",
    "mkdirp": "^0.5.1"
  }
}
