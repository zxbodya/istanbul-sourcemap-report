    "source-map": ">=0.5.6"
  },
  "devDependencies": {
    "codecov.io": "0.1.6",
    "eslint": "^2.2.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.7.0",
    "eslint-plugin-jsx-a11y": "^1.2.0",
    "eslint-plugin-react": "^5.0.1",
    "istanbul": "^0.4.4",
    "jasmine": "^2.4.1",
    "jasmine-core": "^2.4.1",
    "mkdirp": "^0.5.1"
  }
